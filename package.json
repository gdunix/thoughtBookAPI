    "cross-env": "^7.0.3",
    "nodemon": "^2.0.6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "firebase": "^9.17.2",
    "jsonwebtoken": "^8.0.1",
    "mocha": "^8.2.1",
    "mongoose": "^5.11.10",
    "npm": "^6.14.10",
    "winston": "^3.3.3"
  }
