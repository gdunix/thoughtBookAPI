{
  "name": "api",
  "version": "0.3.0",
  "engines": {
    "node": ">=11.0.0"
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "cross-env NODE_ENV=test mocha --recursive",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start:dev": "cross-env NODE_ENV=development babel-watch server.js",
    "start:remote": "yarn build && cross-env NODE_ENV=remote nodemon dist/server.js",
    "start": "yarn build && cross-env NODE_ENV=production node dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.7",
    "cross-env": "^5.0.5",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.15.4",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.0.1",
    "mocha": "^4.0.1",
    "mongoose": "^4.11.9",
    "npm": "^5.4.2",
    "winston": "^3.0.0-rc1"
  }
}
